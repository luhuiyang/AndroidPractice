<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ylh.rxjavaandretrofit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.OnlyUsingRetrofitActivity"
            android:label="retrofit" />
        <activity
            android:name=".activity.UsingRetrofitAndRxJavaActivity"
            android:label="retrofit + rxjava" />
        <activity android:name=".activity.RxJavaExampleActivity" />
        <activity android:name=".activity.HelloWorldActivity" />
        <activity android:name=".activity.FlatMapActivity" />
        <activity android:name=".activity.ErrorScheduleSubscribeActivity" />
        <activity android:name=".activity.DoubleBindingActivity"></activity>
    </application>

</manifest>